{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/pfProver",
            "args": ["tmp/simple_connection.zkey","tmp/simple_connection.fflonkinfo.json","tmp/simple_connection.const","tmp/simple_connection.comt"],
            // "args": ["tmp/simple.zkey","tmp/simple.fflonkinfo.json","tmp/simple.const","tmp/simple.commit"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            // "preLaunchTask": "Build pfProver",
            "miDebuggerPath": "/usr/bin/gdb"
        }

    ]
}